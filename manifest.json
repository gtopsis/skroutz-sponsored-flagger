{
    "name": "Skroutz Sponsored Flagger",
    "description": "Flags sponsored products on Skroutz.gr",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.skroutz.gr/*"
            ],
            "js": [
                "background.js"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "/assets/icons/48.png",
            "128": "/assets/icons/128.png"
        }
    },
    "icons": {
        "48": "/assets/icons/48.png",
        "128": "/assets/icons/128.png"
    }
}